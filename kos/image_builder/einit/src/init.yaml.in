entities:

- name: env.Env
@INIT_Env_ENTITY_CONNECTIONS@

- name: node.Node
  env:
    VFS_NETWORK_BACKEND: @NODE_VFS_NETWORK_BACKEND@
    VFS_FILESYSTEM_BACKEND: client:@ROOTFS_ENTITY@
    PATH: /opt/node/
  @NODE_ARGS@
  connections:
    - target: execution_manager.ExecMgrEntity
      id: ExecMgrEntity
    - target: env.Env
      id: {var: ENV_SERVICE_NAME, include: env/env.h}
    - target: kl.VfsNet
      id: kl.VfsNet
    - target: @ROOTFS_ENTITY@
      id: @ROOTFS_ENTITY@
    - target: kl.TlsEntity
      id: kl.TlsEntity
  @INIT_Node_ENTITY_CONNECTIONS+@

- name: execution_manager.ExecMgrEntity
  path: ExecMgrEntity
  env:
    VFS_FILESYSTEM_BACKEND: client:@ROOTFS_ENTITY@
    LOG_LEVEL: LOG_DEBUG
  connections:
   - target: @ROOTFS_ENTITY@
     id: @ROOTFS_ENTITY@
@INIT_ExecMgrEntity_ENTITY_CONNECTIONS@

@INIT_KLOG_ENTITY@

@INIT_KLOG_STORAGE_ENTITY@

@INIT_EXTERNAL_ENTITIES@
